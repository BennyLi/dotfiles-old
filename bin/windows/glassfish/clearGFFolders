#!/bin/bash

function clear_folder {
  local glassfish folder
  glassfish=$(glassfishRoot "DOS")
  folder="${glassfish}glassfish\\domains\\domain1\\$1\\"
  echo "Deleting the glassfish temporary folder located at $folder"
  comspec "del" "/s" "/f" "/q" "$folder"
  comspec "rd" "/s" "/q" "$folder"
}

clear_folder "generated"
clear_folder "session_store"
clear_folder "osgi-cache"
